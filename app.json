{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vibzworld",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibzworld.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "vibzworld",
            "CFBundleURLSchemes": ["vibzworld"]
          }
        ]
      }
    },
    "android": {
      "package": "com.vibzworld.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "vibzworld"
            },
            {
              "scheme": "https",
              "host": "enter.vibz.world"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-auth-session"],
    "experiments": {
      "typedRoutes": true
    }
  }
}